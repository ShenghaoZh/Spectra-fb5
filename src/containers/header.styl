.root
  flex-grow: 1
  margin-bottom: 0px
  header
    padding-right: 0px

.appbar
  background-color: #404040 !important
  & .home-icon
      margin-right: 0px

.grow
  flex-grow: 1

.titleColor
  color: white

 .rightMenuButton
   button
     padding: 0px
     color: #fff

.rightMenu
  ul
    outline-style: none
    box-shadow: none
    border-color: transparent
    padding: 0px
  li
    color: #404040

.list
  border-right: 2px solid black
  margin: 0px
  height: calc(100vh - 40px)
  width: 240px
  background-color: #FFFFFF
  padding-left: 0px
  padding-right: 0px
  position: fixed
  overflow: scroll
